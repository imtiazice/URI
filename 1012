#include<stdio.h>
#define Pi 3.14159

int main()
{
     double A, B, C, triangle, circle, trapezium, squre, rectangle;
     scanf("%lf %lf %lf", &A, &B, &C);

     triangle = 0.5*A*C;
     circle = Pi*C*C;
     trapezium = 0.5*(A+B)*C;
     squre = B*B;
     rectangle = A*B;
     printf("TRIANGULO: %.3lf\nCIRCULO: %.3lf\nTRAPEZIO: %.3lf\nQUADRADO: %.3lf\nRETANGULO: %.3lf\n", triangle , circle, trapezium, squre, rectangle);

     return 0;
}
